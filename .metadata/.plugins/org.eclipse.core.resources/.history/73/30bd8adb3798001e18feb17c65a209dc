package test;
import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		
		double km, total;
		int starting = 10;
		int min = 20;
		
		System.out.print("Gdilecek km'yi yazınız: ");
		Scanner input = new Scanner(System.in);
		km = input.nextDouble();
		double mesafe = starting +(km*2.20);
		if(mesafe<20) {
			mesafe = 20;
		}else {
			System.out.println("Taksimetre Ücreti = " + mesafe);
		}
		
	}

}
